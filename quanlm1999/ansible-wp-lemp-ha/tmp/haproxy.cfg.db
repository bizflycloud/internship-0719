global
    user haproxy
    group haproxy

defaults
    mode tcp
    log global
    retries 2
    option dontlognull
    timeout connect 3000ms
    timeout server 5000ms
    timeout client 5000ms

listen mysql-cluster
    bind 192.168.122.98:3306
    mode tcp
    option mysql-check user haproxy_check
    balance roundrobin
    server {{db_hostn}} {{db_ip}}:3306 check

